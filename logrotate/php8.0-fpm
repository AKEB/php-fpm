/var/log/php7.1-fpm.log
/var/log/php/*.log
{
        rotate 7
        daily
        missingok
        notifempty
        compress
        delaycompress
        postrotate
                if [ -x /usr/lib/php/php8.0-fpm-reopenlogs ]; then
                        /usr/lib/php/php8.0-fpm-reopenlogs;
                fi
        endscript
}
